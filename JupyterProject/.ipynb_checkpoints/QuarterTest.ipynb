   ],
   "source": [
    "x=pd.Series([2.1,2.3,4.5,2.2,2.5])\n",
    "Q1,Q3=np.percentile(x,[25,75])\n",
    "IQR=Q3-Q1\n",
    "outlier=[]\n",
    "for item in x:\n",
    "    if item <(Q1-1.5*IQR) or  item>(Q3+1.5*IQR):\n",
    "      outlier.append(item)\n",
    "\n",
    "outlier"
   ]
